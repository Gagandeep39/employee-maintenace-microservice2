pipeline {
    agent any
    environment {
        sh 'cd employee-eureka-server'
    }
    stages {
        stage ('Preparation') {
            steps {
                sh 'chmod +x mvnw'
            }
        }
        stage ('Compile') {
            steps {
                sh './mvnw compile'
            }
        }
        stage ('Test') {
            steps {
                sh './mvnw test'
            }
        }
        stage ('Build') {
            steps {
                sh './mvnw clean package'
            }
        }
    }
}
